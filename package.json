{
  "name": "zemuasm-lang",
  "displayName": "ZEmuASM Lang",
  "description": "Synthax highlighting for zemuasm lang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Zevest/zemuasm-lang"
  },
  "contributes": {
    "languages": [{
      "id": "zemuasm",
      "aliases": ["ZEMUASM", "zemuasm", "zemu"],
      "extensions": [".zemu"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "zemuasm",
      "scopeName": "source.zemuasm",
      "path": "./syntaxes/zemuasm.tmLanguage.json"
    }]
  }
}
